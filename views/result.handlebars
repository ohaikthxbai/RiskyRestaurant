<h1>Check your risk:</h1>
<hr>
<div class="container-fluid">
{{!-- <h3>Restaurants:</h3> --}}
 <table class="table table-hover text-left">
    <thead>
      <tr>
        <th>Restaurant Name</th>
        <th>Address</th>
        <th>Risk</th>
      </tr>
    </thead>
    <tbody>
  {{#each restaurant}}
      <tr>
       <td>{{this.DBA_NAME}}</td>
       <td>{{this.ADDRESS}}</td>
       <td>{{this.RISK}}</td>
      </tr>
  {{/each}}
    </tbody>
</table>
</div>

<div class="container-fluid">
<form method="get" action="/">
    <button class="center btn btn-danger" type="submit">Go Back</button>
</form>
</div>